CREATE TABLE CollectionCache (
    id TEXT PRIMARY KEY NOT NULL,
    name TEXT NOT NULL UNIQUE
);

insert:
INSERT OR REPLACE
INTO CollectionCache
VALUES (?,?);

fetchAll:
SELECT *
FROM CollectionCache;

findCollection:
SELECT *
FROM CollectionCache
WHERE name = ?;

renameCollection:
UPDATE CollectionCache
SET name = ?
WHERE id = ?;

delete:
DELETE FROM CollectionCache
WHERE id = ?;