CREATE TABLE CollectionCache (
    id INTEGER AS Int PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL UNIQUE
);

insert:
INSERT OR IGNORE
INTO CollectionCache
VALUES (?,?);

fetchAll:
SELECT *
FROM CollectionCache;

findCollection:
SELECT *
FROM CollectionCache
WHERE name = ?;

renameCollection:
UPDATE CollectionCache
SET name = ?
WHERE id = ?;

delete:
DELETE FROM CollectionCache
WHERE id = ?;